<!DOCTYPE html>
<html lang="en" itemscope itemtype="http://schema.org/WebSite">
<head>
  <title>{{ page_title }} - {{ shop.name }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <meta property="og:title" content="{{ page_title }} - {{ shop.name }}">
  <meta property="og:description" content="{{ page_description | escape }}">
  <meta property="og:type" content="website">
  <meta property="og:url" content="{{ canonical_url }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta property="og:site_name" content="{{ shop.name }}">
  {% render 'og-image' %}
  <link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}">
  <link rel="apple-touch-icon" sizes="180x180" href="{{ settings.favicon.src | img_url: '180x' }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ settings.favicon.src | img_url: '32x' }}">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ settings.favicon.src | img_url: '16x' }}">
  <link rel="manifest" href="{{ 'site.webmanifest' | asset_url }}">
  <link rel="mask-icon" href="{{ 'safari-pinned-tab.svg' | asset_url }}" color="{{ settings.color_brand_mid }}">
  <meta name="msapplication-TileColor" content="{{ settings.color_brand_mid }}">
  <meta name="msapplication-config" content="{{ 'browserconfig.xml' | asset_url }}">
  <meta name="theme-color" content="{{ settings.color_tint3 }}">
  <link rel="preload" as="font" href="{{ settings.heading_font | font_url }}" type="font/woff2" crossorigin>
  {%- if settings.body_font != settings.heading_font -%}
  <link rel="preload" as="font" href="{{ settings.body_font | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_light | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_semi_bold | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_bold | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_light_italic | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_italic | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_semi_bold_italic | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ settings.body_font_bold_italic | font_url }}" type="font/woff2" crossorigin>
  {%- endif -%}
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ 'fonts.css' | asset_url | stylesheet_tag }}
  {{ 'typography.css' | asset_url | stylesheet_tag }}
  {{ content_for_header }}
</head>
<body>
  {% render 'skip-link' %}
  {% render 'logo-nav' %}
  <main id="main">
    {{ content_for_layout }}
  </main>

  {% render 'footer' %}
  <script src="{{ 'application.js' | asset_url }}"></script>
</body>
</html>